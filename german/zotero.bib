  note = {3585011},
  keywords = {0489:Information Technology,0790:Systems science,0984:Computer science,Applied sciences,Budgeting,Computer science,Content analysis,Federal government,Government Accountability Office,Information Technology,Systems science},
  annote = {Copyright - Copyright ProQuest, UMI Dissertations Publishing 2012},
  annote = {First page - n/a},
  annote = {Last updated - 2014-04-30},
